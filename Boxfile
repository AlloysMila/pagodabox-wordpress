web1: 
  name: wp-web 
  shared_writable_dirs:
    - wp-content/
  after_build:
    - "mv pagoda/wp-config.php wp-config.php"
  after_deploy:
    - "cp -R pagoda/wp-content/* wp-content/"
  php_extensions:
    - mysql
    - gd
    - mbstring
    - exif
    - iconv
    - mcrypt
    - zip
db1:
  name: wp-db
  type: mysql